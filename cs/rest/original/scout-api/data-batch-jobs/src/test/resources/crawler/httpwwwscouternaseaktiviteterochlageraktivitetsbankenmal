<script type="text/javascript">
jQuery(document).ready(function() {
	// Create the dropdown base
	jQuery("<select />").appendTo(".menu-main-container");

	// Create default option "Go to..."
	jQuery("<option />", {
	   "selected": "selected",
	   "value"   : "",
	   "text"    : "Huvudmeny"
	}).appendTo(".menu-main-container select");

	// Populate dropdown with menu items
	jQuery(".menu-main-container a").each(function() {
	 var el = jQuery(this);
		jQuery("<option />", {
			"value"   : el.attr("href"),
			"text"    : el.text()
		}).appendTo(".menu-main-container select");
	});
	jQuery(".nav-sub a").each(function() {
	 var el = jQuery(this);
		jQuery("<option />", {
			"value"   : el.attr("href"),
			"text"    : el.text()
		}).appendTo(".menu-main-container select");
	});

	jQuery(".menu-main-container select").change(function() {
  window.location = jQuery(this).find("option:selected").val();
});
});

jQuery(document).ready(function() {
	
		function responsive_sub_nav() {
			var newURL = window.location.protocol + "//" + window.location.host + "/" + window.location.pathname;
			var pathArray = window.location.pathname.split( '/' );
			var secondLevelLocation = pathArray[0];

			var newPathname = "";
		    for ( i = 0; i < pathArray.length; i++ ) {
		      newPathname += "/";
		      newPathname += pathArray[i];
		    }
		    pageName = pathArray[1];

		    jQuery('#header').append('<nav id=\"subaccess\"><div class=\"subnavigation\"></div></nav>');

		    jQuery.get("/", { hip_name: pageName })
		        .done(function(data) {
		           var menuTitle = data;
		        	// Create the dropdown base
		        	if(jQuery("#sidebar .menu a").length){
		        		jQuery("<select />").appendTo(".subnavigation");

		        		// Create default option "Go to..."
		        		jQuery("<option />", {
		        		   "selected": "selected",
		        		   "value"   : "",
		        		   "text"    : "Meny för" + menuTitle,
		        		}).appendTo(".subnavigation select");

		        		// Populate dropdown with menu items
		        		jQuery("#sidebar .menu a").each(function() {
		        		 var el = jQuery(this);
		        			jQuery("<option />", {
		        				"value"   : el.attr("href"),
		        				"text"    : el.text()
		        			}).appendTo(".subnavigation select");
		        		});

		        		jQuery(".subnavigation select").change(function() {
		        			window.location = jQuery(this).find("option:selected").val();
		        		});
		        	}
		        	if(jQuery("#sidebar .submenu a").length){
		        		jQuery("<select />").appendTo(".subnavigation");

		        		// Create default option "Go to..."
		        		jQuery("<option />", {
		        		   "selected": "selected",
		        		   "value"   : "",
		        		   "text"    : "Meny för " + menuTitle,
		        		}).appendTo(".subnavigation select");

		        		// Populate dropdown with menu items
		        		jQuery("#sidebar .submenu a").each(function() {
		        		 var el = jQuery(this);
		        			jQuery("<option />", {
		        				"value"   : el.attr("href"),
		        				"text"    : el.text()
		        			}).appendTo(".subnavigation select");
		        		});

		        		jQuery(".subnavigation select").change(function() {
		        			window.location = jQuery(this).find("option:selected").val();
		        		});
		        	}
		    });
			
		}
		
		if ( jQuery( window ).width() < 800 ) {
			responsive_sub_nav();	
		}	
	});	
	
</script>
